<idea-plugin>
    <id>org.dandoh.ideaforces</id>
    <name>Ideaforces</name>
    <vendor email="dainhan605@gmail.com" url="https://github.com/dandoh">Dandoh</vendor>
    <idea-version since-build="190.3000"/>

    <description>
        This plugin allows you to have a collection of favorite IDE Actions and choose from it.
    </description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <extensions defaultExtensionNs="com.intellij">
        <projectService id="FavoriteActionsService"
                        serviceImplementation="org.dandoh.ideaforces.services.IdeaforcesService"/>
    </extensions>

    <actions>
    </actions>


</idea-plugin>